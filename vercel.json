{
    "version": 2,
    "scope":"server",
    "builds": [
    {
      "src": "/dist/apps/server/main.js",
      "use": "@vercel/node"
    }],
    "routes": [
        { "src": "/api/(.*)", "dest": "/dist/apps/server/main.js",  
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
  }
    ]
}


