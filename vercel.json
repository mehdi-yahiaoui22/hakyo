{
    "version": 2,
    "scope":"hakyoapi",
    "builds": [
    {
      "src": "dist/apps/hakyoapi/main.js",
      "use": "@now/node"
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/apps/hakyoapi/main.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
 
        }]
}


